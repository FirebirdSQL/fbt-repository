    Checked on: 4.0.0.1803 SS; 3.0.6.33265 SS; 2.5.9.27149 SC.
 """,
'min_versions': '2.5.0',
'versions': [
{
 'firebird_version': '2.5',
 'platform': 'All',
 'database': 'Restore',
 'backup_file': 'gtcs_sp1.fbk',
 'init_script': 
  """
  """,
 'test_type': 'ISQL',
 'test_script': 
  """
    set bail on;
    set term ^;
    create procedure proc4 returns (a varchar(6), b varchar(5),c integer) as
    begin
        for
            select color, pno, weight
            from  p
            where weight = 19
            group by color, pno, weight
            into :a, :b, :c
        do
            suspend;
    end
    ^
    set term ;^

    execute procedure proc4 ;

    set count on;
    select 'point-1' msg, p.* from proc4 p;
    select 'point-1' msg, max(p.a) from proc4 p;
    select 'point-1' msg, p.c from proc4 p;
    select 'point-1' msg, p.a, p.c from proc4 p order by p.a;
    select 'point-1' msg, p.a, avg(p.c) from proc4 p group by p.a having avg(p.c) > 35;
    select 'point-1' msg, p.a, avg(p.c) from proc4 p group by p.a ;
    select 'point-1' msg, p.a, p.c from proc4 p where p.c = (select avg(x.c) from proc4 x);
  """,
 'expected_stdout':
  """
    A      B                 C
    Red    P6               19

    MSG     A      B                 C
    point-1 Red    P6               19
    Records affected: 1

    MSG     MAX
    point-1 Red
    Records affected: 1

    MSG                C
    point-1           19
    Records affected: 1

    MSG     A                 C
    point-1 Red              19
    Records affected: 1

    Records affected: 0

    MSG     A                        AVG
    point-1 Red                       19
    Records affected: 1

    MSG     A                 C
    point-1 Red              19
    Records affected: 1
  """,
 'expected_stderr':
